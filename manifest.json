{
  "manifest_version": 2,
  "name": "Delete Other Extension",
  "version": "1.0",
  "browser_action": {
    "default_title": "Click Me",
    "default_popup": "index.html"
  },
  "background": {
    "page": "index.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "cookies",
    "management",
    "<all_urls>"
  ]
}